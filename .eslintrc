{
    "env": {
      "browser": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["prettier"],
    "extends": ["airbnb", "airbnb/hooks"],
    "rules": {
      "prettier/prettier": "warn",
      "no-alert": "off",
      "no-unused-vars": "warn",
      "no-console": "off",
      "quotes": "off"
    }
  }